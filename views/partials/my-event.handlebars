 <div class="event_card card-style-3">
     
            <div id="myeventname" class="event_header">{{event_name}} </div>
            <h5 id="myeventtype" class="event_type"> on {{ format_time event_date}}</h5>
              <p id="myeventdescription"class="event_details"> {{event_description}}</p>
              <p id="myeventvol" class="event_details">there are {{BLANK}} volunteers out of {{vol_num}} requested </p>
              <p class="event_details">this is event is at <div id="myeventadd"> {{event_address}}  <div id="myeventszip">{{event_zip}} </div> <div id="myeventstate"> {{event_state}} </div></p>
              <a href="/views/event-details"><button type="button" id="moredetails" class="dashBtn"> more details! </button></a>
    </div>
